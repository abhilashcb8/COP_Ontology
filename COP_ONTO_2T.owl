@prefix : <http://www.semanticweb.org/hp/ontologies/2021/COP#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/hp/ontologies/2021/COP> .

<http://www.semanticweb.org/hp/ontologies/2021/COP> rdf:type owl:Ontology ;
                                                     rdfs:comment """The COP ontology is designed to understand the type of infection or the symptoms that are recorded in all teh pharmacy in karnataka state. 
The district wise symptom / infctions can be arranged to see and analyse the impact and take necessary precuations to act upon the pandemic situations."""@en ;
                                                     rdfs:label "COP - COVID-19 Ontology for Pharma"@en .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/hp/ontologies/2021/COP#confirms
:confirms rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:label "confirms"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#diagnosis
:diagnosis rdf:type owl:ObjectProperty ;
           rdfs:domain :Diagnosis ;
           rdfs:range [ rdf:type owl:Restriction ;
                        owl:onProperty :diagnosis ;
                        owl:someValuesFrom xsd:string
                      ] .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#getPrescription
:getPrescription rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:label "getPrescription"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasAllergy
:hasAllergy rdf:type owl:ObjectProperty ;
            owl:equivalentProperty <http://www.semanticweb.org/hp/ontologies/2021/COP#hasAllergy+cough> ,
                                   <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy> ,
                                   <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy+cough> ;
            rdfs:subPropertyOf :diagnosis .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasCough
:hasCough rdf:type owl:ObjectProperty ;
          owl:equivalentProperty <http://www.semanticweb.org/hp/ontologies/2021/COP#hasAllergy+cough> ,
                                 <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+cough> ,
                                 <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy+cough> ;
          rdfs:subPropertyOf :diagnosis .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasDiagnoisedFor
:hasDiagnoisedFor rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :diagnosis ;
                  owl:inverseOf :isDiagnoisedFor ;
                  rdfs:domain :Diagnosis ;
                  rdfs:range :Symptoms ;
                  rdfs:label "hasDiagnoisedFor"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever
:hasFever rdf:type owl:ObjectProperty ;
          owl:equivalentProperty <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy> ,
                                 <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+cough> ,
                                 <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy+cough> ;
          rdfs:subPropertyOf :diagnosis .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasPharmacist
:hasPharmacist rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Drug_Store ;
               rdfs:range :Pharmacist ;
               rdfs:label "hasPharmacist"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasSymptom
:hasSymptom rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:label "hasSymptom"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#isDiagnoisedFor
:isDiagnoisedFor rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :diagnosis ;
                 rdfs:label "isDiagnoisedFor"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#medicineFor
:medicineFor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:label "medicineFor"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#procuredOn
:procuredOn rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:label "procuredOn"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#provides
:provides rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:label "provides"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#providesMedicines
:providesMedicines rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:label "providesMedicine"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#residesAt
:residesAt rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:label "residentAt"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#takenTo
:takenTo rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:label "takenTo"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#underGo
:underGo rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:label "underGO"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#wentTo
:wentTo rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:comment "wentTo"^^xsd:string ;
        rdfs:label "wentTo"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasAllergy+cough
<http://www.semanticweb.org/hp/ontologies/2021/COP#hasAllergy+cough> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf :diagnosis .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy
<http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf :diagnosis .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+cough
<http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+cough> rdf:type owl:ObjectProperty ;
                                                                   rdfs:subPropertyOf :diagnosis .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy+cough
<http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy+cough> rdf:type owl:ObjectProperty ;
                                                                           rdfs:subPropertyOf :diagnosis .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#hasILI/SARI
<http://www.semanticweb.org/hp/ontologies/2021/COP#hasILI/SARI> rdf:type owl:ObjectProperty ;
                                                                rdfs:subPropertyOf :diagnosis .


