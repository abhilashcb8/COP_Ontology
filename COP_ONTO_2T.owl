#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/hp/ontologies/2021/COP#Address
:Address rdf:type owl:Class ;
         rdfs:subClassOf :Location ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :address ;
                           owl:someValuesFrom xsd:string
                         ] ;
         rdfs:label "Address"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Agent
:Agent rdf:type owl:Class .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Allergy
:Allergy rdf:type owl:Class ;
         rdfs:subClassOf :Symptoms ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :Allergy ;
                           owl:someValuesFrom xsd:string
                         ] .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Cough
:Cough rdf:type owl:Class ;
       rdfs:subClassOf :Symptoms ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :Cough ;
                         owl:someValuesFrom xsd:string
                       ] .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Date
:Date rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :procuredOn ;
                        owl:someValuesFrom :Date
                      ] ;
      rdfs:label "Date"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Diagnosis
:Diagnosis rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasAllergy ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasCough ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasFever ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+cough> ;
                             owl:someValuesFrom xsd:string
                           ] ;
           rdfs:label "Diagnosis"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#District
:District rdf:type owl:Class ;
          rdfs:subClassOf :Address ;
          rdfs:label "District"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Doctor
:Doctor rdf:type owl:Class ;
        rdfs:subClassOf :HealthFacility ,
                        :Person ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :confirms ;
                          owl:someValuesFrom :Symptoms
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :prescription ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ;
        owl:disjointWith :Patient ,
                         :Pharmacist ;
        rdfs:label "Doctor"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Drug_Store
:Drug_Store rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasPharmacist ;
                              owl:someValuesFrom :Pharmacist
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :drugLocation ;
                              owl:someValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :drugStoreName ;
                              owl:someValuesFrom xsd:string
                            ] ;
            rdfs:label "Drug store"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Drug_Type
:Drug_Type rdf:type owl:Class ;
           owl:equivalentClass :Symptoms ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :medicineFor ;
                             owl:someValuesFrom :Diagnosis
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasAllergy ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasCough ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasFever ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.semanticweb.org/hp/ontologies/2021/COP#hasAllergy+cough> ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy> ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+cough> ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.semanticweb.org/hp/ontologies/2021/COP#hasFever+allergy+cough> ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.semanticweb.org/hp/ontologies/2021/COP#hasILI/SARI> ;
                             owl:someValuesFrom xsd:string
                           ] ;
           rdfs:label "Drug_Type"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Fever
:Fever rdf:type owl:Class ;
       rdfs:subClassOf :Symptoms ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :Fever ;
                         owl:someValuesFrom xsd:string
                       ] .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#ForcedExpiratoryVolume
:ForcedExpiratoryVolume rdf:type owl:Class ;
                        rdfs:subClassOf :Diagnosis ;
                        rdfs:label "ForcedExpiratoryVolume"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#HealthFacility
:HealthFacility rdf:type owl:Class .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#HeartRate
:HeartRate rdf:type owl:Class ;
           rdfs:subClassOf :Diagnosis .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Location
:Location rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :residesAt ;
                            owl:someValuesFrom :Location
                          ] ;
          rdfs:label "Location"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#PID
:PID rdf:type owl:Class ;
     rdfs:subClassOf :Patient .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Patient
:Patient rdf:type owl:Class ;
         rdfs:subClassOf :Person ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasDiagnoisedFor ;
                           owl:someValuesFrom :Diagnosis
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasSymptom ;
                           owl:someValuesFrom :Symptoms
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :residesAt ;
                           owl:someValuesFrom :Location
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :takenTo ;
                           owl:someValuesFrom :HealthFacility
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :wentTo ;
                           owl:someValuesFrom :HealthFacility
                         ] ;
         rdfs:label "Patient"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf :Agent ;
        rdfs:label "Person"^^xsd:string .


###  http://www.semanticweb.org/hp/ontologies/2021/COP#Pharmacist
:Pharmacist rdf:type owl:Class ;
            rdfs:subClassOf :Person ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :provides ;
                              owl:someValuesFrom :Drug_Type
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :providesMedicines ;
                              owl:someValuesFrom :Drug_Type
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :providesMedicines ;
